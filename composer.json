{
    "name": "sonar/valiable",
    "description": "sonar valiables",
    "keywords": [
        "sonar",
        "valiable"
    ],
    "homepage": "https://github.com/dev-sonar/sonar-valiable",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuichi Matsumoto",
            "email": "matsumoto@snr.jp",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/console": "^6.0",
        "illuminate/cache": "^6.0",
        "illuminate/filesystem": "^6.0",
        "sonar/common": "^2.0",
        "php" : ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Sonar\\Valiable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonar\\Valiable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "master": "1.0-dev"
        }
    }
}
